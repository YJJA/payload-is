      "default": "./lib/commonjs/url.cjs"
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "typescript",
    "javascript",
    "type-check",
    "type-guard",
    "validation",
    "array",
    "object",
    "primitive",
    "collections",
    "data-types",
    "type-testing"
  ],
  "homepage": "https://github.com/YJJA/payload-is.git#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/YJJA/payload-is.git"
  },
  "bugs": {
