{
  "name": "payload-is",
  "version": "0.3.3",
  "description": "javascript data check, supported typescript",
  "type": "module",
  "main": "./lib/commonjs/index.cjs",
  "module": "./lib/esm/index.js",
  "types": "./lib/types/index.d.ts",
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/types/index.d.ts",
        "default": "./lib/esm/index.js"
      },
      "require": {
        "types": "./lib/types/index.d.ts",
        "default": "./lib/commonjs/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build:types": "tsc -p tsconfig.types.json",
    "build:js": "rollup -c",
    "build": "pnpm run clean && pnpm run build:types && pnpm run build:js",
    "check-types": "tsc --noEmit",
    "clean": "del lib",
    "test": "node --test './src/__tests__/*.test.*'",
    "prepublishOnly": "pnpm run test && pnpm run build"
  },
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__test_utils__"
  ],
  "keywords": [
    "javascript type check",
    "typescript data check",
    "isObject",
    "isPlainObject"
  ],
  "homepage": "https://github.com/YJJA/payload-is.git#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/YJJA/payload-is.git"
  },
  "bugs": {
    "url": "https://github.com/YJJA/payload-is.git/issues"
  },
  "author": {
    "name": "Liu Hong",
    "email": "hongdoit@icloud.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/plugin-transform-runtime": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@types/node": "^22.14.0",
    "del-cli": "^6.0.0",
    "glob": "^11.0.2",
    "rollup": "^4.41.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.4"
  }
}
