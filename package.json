  },
  "license": "MIT",
  "devDependencies": {
    "@tscb/builder-esbuild": "^0.2.1",
    "@tscb/tsconfig": "^0.2.1",
    "@types/node": "^22.14.0",
    "del-cli": "^6.0.0",
    "prettier": "3.5.3",
    "typescript": "^5.8.3"
  }
}